<p id="notice"><%= notice %></p>
<h1>brands</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @menus.each do |menu| %>
      <% @menus.erros.messages %>
      <tr>
        <td><%= menu.name %> - <%= menu.id %></td>
        <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
        <td><%= link_to 'Destroy', menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Menu', new_menu_path %>